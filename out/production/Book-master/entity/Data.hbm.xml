<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="entity.Data" table="data">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		
		<property name="name" column="name" type="java.lang.String"></property>
		<property name="user" column="user" type="java.lang.String"></property>
		<property name="bid" column="bid" type="int"></property>
		<property name="createDate" column="createDate" type="java.util.Date"></property>
		<property name="bookReturn" column="bookReturn" type="java.util.Date"></property>
	</class>
</hibernate-mapping>